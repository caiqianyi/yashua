<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.lebaoxun.yashua.modules.yashua.dao.UserDataDao">

<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.lebaoxun.yashua.modules.yashua.entity.UserDataEntity" id="userDataMap">
        <result property="id" column="id"/>
        <result property="fenshu" column="fenshu"/>
        <result property="biaoshi" column="biaoshi"/>
        <result property="adddate" column="adddate"/>
        <result property="userid" column="userid"/>
    </resultMap>

	<select id="lookList" resultType="com.lebaoxun.yashua.modules.yashua.entity.UserDataEntity">
	    select * from user_data where userid=#{userid} and DATE_FORMAT(adddate,'%Y-%m-%d 00:00:00')=#{adddate} and biaoshi=#{biaoshi}
	</select>
</mapper>